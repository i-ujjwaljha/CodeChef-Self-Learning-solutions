#include <bits/stdc++.h>
using namespace std;

int main() {
   int t;
   cin>>t;
   while(t--)
   {
       char n;
        cin>>n;
       string arr[4]={"BattleShip","Cruiser","Destroyer","Frigate"};
   
     if(n=='B'||n=='b')
       cout<<"BattleShip"<<endl;
   
     else if(n=='C'||n=='c')
       cout<<"Cruiser"<<endl;
   
     else if(n=='D'||n=='d')
       cout<<"Destroyer"<<endl;
   
     else
       cout<<"Frigate"<<endl;
   
   }
	return 0;
}
